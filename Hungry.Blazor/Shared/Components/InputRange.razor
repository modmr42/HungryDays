<input class="form-range form-control" type="range" min="@Min" max="@Max" step="@Step" name="@Name" />


@code{
    [Parameter]
    public int Min { get; set; }

    [Parameter]
    public int Max { get; set; }

    [Parameter]
    public int Step { get; set; }

    [Parameter]
    public string Name { get; set; }

}